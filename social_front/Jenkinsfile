node {
    // Checkout Repository A
    checkout([$class: 'GitSCM', branches: [[name: '*/master']], userRemoteConfigs: [[url: 'https://github.com/MellouliiAhmed/Jenkins_Central.git']]])

    // Load and execute the Jenkinsfile from Repository A
    def centralJenkinsfile = load 'Jenkins-Social-Frontend'

    // Execute the pipeline
    centralJenkinsfile()
}


